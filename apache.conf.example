WSGIDaemonProcess DR-3araby.github.io user=3araby group=3araby threads=4 python-home=/path/to/DR-3araby.github.io/venv/

<VirtualHost *:80>
     ServerName 3araby.me
     ServerAlias www.3araby.me

     WSGIScriptAlias / /path/to/DR-3araby.github.io/personal_website.wsgi
     <Directory /path/to/yshalsager_website/>
            WSGIScriptReloading On
            WSGIProcessGroup yshalsager_website
            WSGIApplicationGroup %{GLOBAL}
            Options FollowSymLinks
            AllowOverride None
            Require all granted
     </Directory>
     ErrorLog /path/to/DR-3araby.github.io/logs/error.log
     LogLevel warn
     CustomLog /path/to/DR-3araby.github.io/logs/access.log combined
RewriteEngine on
RewriteCond %{SERVER_NAME} =www.3araby.me [OR]
RewriteCond %{SERVER_NAME} =3araby.me
RewriteRule ^ https://%{SERVER_NAME}%{REQUEST_URI} [END,NE,R=permanent]
</VirtualHost>

<VirtualHost *:443>
    ServerName 3araby.me
    ServerAlias www.3araby.me

     WSGIScriptAlias / /path/to/DR-3araby.github.io/personal_website.wsgi
     <Directory /path/to/DR-3araby.github.io/>
            WSGIScriptReloading On
            WSGIProcessGroup yshalsager_website
            WSGIApplicationGroup %{GLOBAL}
            Options FollowSymLinks
            AllowOverride None
            Require all granted
     </Directory>
     ErrorLog /path/to/DR-3araby.github.io/logs/error.log
     LogLevel warn
     CustomLog /path/to/DR-3araby.github.io/logs/access.log combined

RewriteEngine on
# Some rewrite rules in this file were disabled on your HTTPS site,
# because they have the potential to create redirection loops.

# RewriteCond %{SERVER_NAME} =xfu.xiaomifirmwareupdater.com [OR]
# RewriteCond %{SERVER_NAME} =www.xfu.xiaomifirmwareupdater.com
# RewriteRule ^ https://%{SERVER_NAME}%{REQUEST_URI} [END,NE,R=permanent]
Include /etc/letsencrypt/options-ssl-apache.conf
SSLCertificateFile /etc/letsencrypt/live/yshalsager.me/fullchain.pem
SSLCertificateKeyFile /etc/letsencrypt/live/yshalsager.me/privkey.pem
</VirtualHost>

